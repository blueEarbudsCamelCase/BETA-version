{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/proxy",
        "function": "proxy"
      },
      {
        "source": "/proxy/**",
        "function": "proxy"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
